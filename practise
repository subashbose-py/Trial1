      "source": [
        "import json\n",
        "from tabulate import tabulate\n",
        "# feel free to add more, if you need to\n",
        "with open('/content/sample_data/hfxbus.json') as f:\n",
        "  data = f.read()\n",
        "\n",
        "json_dict = json.loads(data)\n",
        "\n",
        "print(\"The JSON document is loaded as type {0}\\n\".format(type(json_dict)))\n",
        "\n",
        "\n",
        "header = [\"route_id\", \"route_short_name\", \"route_long_name\"] \n",
        "new=[\"route_desc\", \"route_url\",\"agency_id\"]\n",
        "rev_dic=None\n",
        "a=[]\n",
        "\n",
        "for new_dict in json_dict:\n",
        "  for key,value in new_dict.items(): \n",
        "    if key not in new:\n",
        "        rev_dict ={key:value}\n",
        "        a.append(rev_dict)\n",
        "print(a)\n",
        "v= dict(zip(header,header))\n",
        "\n",
        "print(tabulate(a,v,tablefmt=\"github\"))\n",
        "\n",
        "\n"
      ]
    },
